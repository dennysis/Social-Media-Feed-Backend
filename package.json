{
    "name": "social-media-backend",
    "version": "1.0.0",
    "description": "A basic social media feed backend using Node.js, Express, GraphQL, Prisma, and PostgreSQL.",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "test": "NODE_ENV=test jest --detectOpenHandles",
        "test:watch": "NODE_ENV=test jest --watch",
        "test:coverage": "NODE_ENV=test jest --coverage"
    },
    "dependencies": {
        "@apollo/server": "^4.11.3",
        "@prisma/client": "^4.16.2",
        "bcrypt": "^5.1.1",
        "body-parser": "^2.2.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.21.2",
        "graphql": "^16.10.0",
        "graphql-tag": "^2.12.6",
        "jsonwebtoken": "^9.0.2",
        "long": "^5.3.1",
        "mime": "^3.0.0",
        "prisma": "^4.16.2"
    },
    "devDependencies": {
        "@types/accepts": "^1.3.7",
        "@types/bcrypt": "^5.0.2",
        "@types/body-parser": "^1.19.5",
        "@types/connect": "^3.4.38",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-serve-static-core": "^5.0.6",
        "@types/http-errors": "^2.0.4",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/long": "^5.0.0",
        "@types/mime": "^4.0.0",
        "@types/ms": "^2.1.0",
        "@types/node": "^22.14.0",
        "@types/node-fetch": "^2.6.12",
        "@types/qs": "^6.9.18",
        "@types/range-parser": "^1.2.7",
        "@types/send": "^0.17.4",
        "@types/serve-static": "^1.15.7",
        "jest": "^29.7.0",
        "nodemon": "^2.0.20",
        "ts-jest": "^29.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    }
}
